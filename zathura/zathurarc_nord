#背景颜色
set default-bg \#2e3440
set default-fg \#d8dee9
#状态栏颜色
set statusbar-bg \#2e3440
set statusbar-fg \#d8dee9
#输入栏颜色
set inputbar-bg \#2e3440
set inputbar-fg \#d8dee9
#补全颜色
set completion-bg \#2e3440
set completion-fg \#d8dee9
set completion-group-bg \#88c0d0
set completion-group-fg \#2e3440
set completion-highlight-bg \#81a1c1
set completion-highlight-fg \#d8dee9
#tab颜色
set tabbar-bg \#2e3440
set tabbar-fg \#d8dee9
set tabbar-focus-bg \#81a1c1
set tabbar-focus-fg \#d8dee9
#index颜色
set index-bg \#2e3440
set index-fg \#d8dee9
set index-active-bg \#81a1c1
set index-active-fg \#d8dee9
#反色颜色
set recolor true
set recolor-darkcolor \#d8dee9
